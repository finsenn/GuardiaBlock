{
    "name": "GuardiaBlock",
    "version": "1.0.0",
    "manifest_version": 3,
    "permissions": [
        "declarativeNetRequest"
    ],
    "declarative_net_request":{
        "rule_resources":[
            {
                "id": "ruleset_1",
                "enabled": true,
                "path": "rules.json"
            }
        ]
    },
    "icons": {
        "16": "icons/guardiablock_16x16.png",
        "48": "icons/guardiablock_48x48.png",
        "128": "icons/guardiablock_128x128.png"
    }

}